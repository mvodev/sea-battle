{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "@typescript-eslint/quotes": [
  "error",
  "single",
  {
    "allowTemplateLiterals": true
  }
],
  "rules": {
    "import/prefer-default-export": "off"
  },
  "ignorePatterns": ["dist", "gulp"]
}
